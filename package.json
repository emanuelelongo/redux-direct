{
  "name": "redux-reduced",
  "version": "1.0.5",
  "description": "Redux plugin that simplify the way you write actions",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "babel src -d dist --copy-files --ignore test",
    "prepublish": "npm run build",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf coverage && rm -rf example/.next && rm -rf example/node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emanuelelongo/redux-reduced.git"
  },
  "keywords": [
    "react",
    "redux",
    "redux-reduced",
    "redux state",
    "redux simple"
  ],
  "author": "Emanuele Longo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emanuelelongo/redux-reduced/issues"
  },
  "homepage": "https://github.com/emanuelelongo/redux-reduced#readme",
  "dependencies": {
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^3.0.0",
    "jest": "^21.2.1"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "coverageDirectory": "./coverage/"
  }
}
